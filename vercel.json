{
  "version": 2,
  "builds": [
    { "src": "api/*.ts", "use": "@vercel/node" },
    { "src": "*.ts", "use": "@vercel/node" },
    { "src": "/*.ts", "use": "@vercel/node" },
    { "src": "*", "use": "@vercel/static-build" },
    { "src": "/*", "use": "@vercel/static-build" }
  ],
  "routes": [{ "src": "/api/(.*)", "dest": "api/$1" }]
}
